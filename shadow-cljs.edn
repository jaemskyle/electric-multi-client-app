{
 :builds
 ; https://shadow-cljs.github.io/docs/UsersGuide.html#_output_settings
 ; :output-dir - The directory to use for all compiler output.
 ; :asset-path - The relative path from web serverâ€™s root to the resources in :output-dir.
 ; web server must serve resources/public as its root
 {:app-dev  {:deps {:aliases [:app]}
                 :target :browser
                 :devtools {:loader-mode :default
                            :repl-pprint true}
                 :output-dir "resources/public/app/js"
                 :asset-path "/app/js"
                 :modules {:main {:entries [app-dev] :init-fn app-dev/start!}}
                 :build-hooks [(hyperfiddle.electric.shadow-cljs.hooks/reload-clj)]}
  :app-prod {:target :browser
                 :output-dir "resources/public/app/js"
                 :asset-path "/app/js"
                 :modules {:main {:entries [app-prod] :init-fn app-prod/start!}}
                 :module-hash-names true}
  :admin-dev  {:deps {:aliases [:admin]}
               :target :browser
               :devtools {:loader-mode :default
                          :repl-pprint true}
               :output-dir "resources/public/admin/js"
               :asset-path "/admin/js"
               :modules {:main {:entries [admin-dev] :init-fn admin-dev/start!}}
               :build-hooks [(hyperfiddle.electric.shadow-cljs.hooks/reload-clj)]}
  :admin-prod {:target :browser
               :output-dir "resources/public/admin/js"
               :asset-path "/admin/js"
               :modules {:main {:entries [admin-prod] :init-fn admin-prod/start!}}
               :module-hash-names true}}}
